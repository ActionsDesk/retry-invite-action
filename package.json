{
  "name": "retry-invite-action",
  "version": "1.0.0",
  "description": "Retries Failed User Invites",
  "main": "src/main.js",
  "scripts": {
    "test": "jest --collectCoverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "pack": "tsc && ncc build && rm -f src/*.js",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx npm-run-all format lint test",
      "pre-push": "npx npm-run-all format lint test"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "@zeit/ncc": "^0.21.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "octomock": "git+https://github.com/Chocrates/octomock.git",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^2.0.1",
    "outdent": "^0.7.0"
  },
  "author": "Chris McIntosh (Chocrates@github.com)",
  "license": "MIT"
}
